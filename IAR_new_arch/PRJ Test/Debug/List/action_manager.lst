     39            U0BAUD = 0;  // табличные значения из pdf
     40            U0GCR =  16;
     41            // Включаем альтернативные функции выводов
     42            P0SEL = (1<<2)|(1<<3);
     43            U0CSR |= (1<<6);
     44          }
