    106          /**
    107          @brief Получить указатель на последний элемент в цепочке
    108          @param[in] fr указатель на структуру frame
    109          @return указатель на последний буфер или NULL
    110          */
    111          fbuf_s* frame_get_fbuf_tail(frame_s *fr)
    112          {
    113            return fr->tail;
    114          }
