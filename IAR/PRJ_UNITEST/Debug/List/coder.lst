     83            free(fb);
     84          }
     85          
     86          /**
     87          @brief Переход к следующему буферу
     88          @return Указатель на следующий буфер или NULL
     89          */
     90          fbuf_s* fbuf_next(fbuf_s *fb)
     91          {
     92            if (fb->next == NULL)
