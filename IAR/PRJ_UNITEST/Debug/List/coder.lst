    322          static inline void setFreq(uint8_t CH)
    323          {
    324            ASSERT_HALT( (CH >= 11) && (CH <= 26), "Incorrect radio channel");
    325            // Устанавливаем частоту радиопередатчика
    326            FREQCTRL_u FRQ;
    327            FRQ.value = FRQ_CALC(RADIO_CFG.CH);
    328            FREQCTRL = FRQ.value;  
    329          }
