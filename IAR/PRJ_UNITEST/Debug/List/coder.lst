    246          static inline void Callback_execution(void (*fn)(uint8_t TS), uint8_t TS)
    247          {
    248            // Помошник вызова функций. Упрощает проверки
    249            ASSERT_HALT(fn != NULL, "Callback is NULL");
    250            if (fn == NULL)
    251              return;
    252            fn(TS);
    253          }
