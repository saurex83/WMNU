    420            uint8_t block_len = len % 16; // Размер последнего блока
    421            uint8_t ptr; // Смещение
    422                
    423            // Для этого типа шифрования длина блоков по 16 байт
    424            ST_DEF(DMA_AES_DW, LENL, 16);
