    426            ST_DEF(DMA_AES_UP, DSTADDRL, LADDR(mac));
    427            ST_DEF(DMA_AES_UP, DSTADDRH, HADDR(mac));
    428            ST_DEF(DMA_AES_UP, LENL, 16);
